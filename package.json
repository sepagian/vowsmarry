{
	"name": "vowsmarry-v2",
	"private": true,
	"version": "0.3.0",
	"type": "module",
	"scripts": {
		"dev": "doppler run -- vite dev",
		"build": "doppler run -- vite build",
		"preview": "doppler run -- vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest",
		"test:run": "vitest --run",
		"release": "release-it",
		"db:push": "doppler run -- drizzle-kit push",
		"db:generate": "doppler run -- drizzle-kit generate",
		"db:migrate": "doppler run -- drizzle-kit migrate",
		"db:studio": "doppler run -- drizzle-kit studio"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
