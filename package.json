		"drizzle-kit": "^0.31.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"formsnap": "^2.0.1",
		"globals": "^16.0.0",
		"jsdom": "^27.0.0",
		"mode-watcher": "^1.1.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"release-it": "^19.0.5",
		"svelte": "^5.0.0",
