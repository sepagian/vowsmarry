{
  "name": "vowsmarry-v2",
  "private": true,
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "bunx wrangler pages dev --compatibility-date=2025-11-20 --d1=vowsmarry -- vite dev",
    "dev:vite": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test": "vitest",
    "test:run": "vitest --run",
    "release": "release-it",
    "db:push": "bun run drizzle-kit push",
    "db:generate": "bun run drizzle-kit generate",
    "db:migrate": "bunx wrangler d1 migrations apply vowsmarry --local",
    "db:migrate:remote": "bunx wrangler d1 migrations apply vowsmarry --remote",
    "db:studio": "bun run drizzle-kit studio",
    "db:shell": "bunx wrangler d1 execute vowsmarry --local --command",
    "db:shell:remote": "bunx wrangler d1 execute vowsmarry --remote --command"
  },
  "devDependencies": {
