		"@unocss/reset": "^66.5.0",
		"clsx": "^2.1.1",
		"d3-array": "^3.2.4",
		"d3-scale": "^4.0.2",
		"drizzle-orm": "^0.44.5",
		"layerchart": "2.0.0-next.27",
		"postgres": "^3.4.5",
