		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tanstack/table-core": "^8.20.5",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-shape": "^3.1.7",
		"@types/node": "^22",
		"@unovis/svelte": "^1.6.1",
